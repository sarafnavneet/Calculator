<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <LinearLayout

        android:id="@+id/linearLayout"
        android:layout_width="400dp"
        android:layout_height="484dp"

        android:layout_marginLeft="25dp"
        android:layout_marginTop="6dp"
        android:layout_marginRight="25dp"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout2"
        android:baselineAligned="false">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/imageView1"
                android:layout_width="match_parent"
                android:layout_height="150dp"
                android:layout_weight="1"
                app:srcCompat="@drawable/bulb"
                tools:ignore="NestedWeights" />

            <ImageView
                android:id="@+id/imageView2"
                android:layout_width="match_parent"
                android:layout_height="150dp"
                android:layout_weight="1"
                app:srcCompat="@drawable/tubelight" />

            <ImageView
                android:id="@+id/imageView3"
                android:layout_width="match_parent"

                android:layout_height="150dp"
                android:layout_weight="1"
                app:srcCompat="@drawable/fan" />

            <ImageView
                android:id="@+id/imageView4"
                android:layout_width="match_parent"

                android:layout_height="150dp"
                android:layout_weight="1"
                app:srcCompat="@drawable/tv" />

            <ImageView
                android:id="@+id/imageView5"
                android:layout_width="match_parent"
                android:layout_height="150dp"
                android:layout_weight="1"
                app:srcCompat="@drawable/crttv" />

            <ImageView
                android:id="@+id/imageView6"
                android:layout_width="match_parent"

                android:layout_height="150dp"
                android:layout_weight="1"
                app:srcCompat="@drawable/fridge" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:orientation="vertical">


            <EditText
                android:id="@+id/a1"
                android:layout_width="match_parent"
                android:layout_height="73dp"
                android:ems="10"
                android:gravity="center_horizontal"
                android:hint="6"
                android:inputType="textPersonName"
                android:textAlignment="center" />

            <CheckBox
                android:id="@+id/c1"
                android:layout_width="match_parent"
                android:layout_height="20dp"
                android:text="@string/Dont_Know" />

            <EditText
                android:id="@+id/a2"
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:layout_weight="1"
                android:ems="10"
                android:gravity="center_horizontal"
                android:hint="5"
                android:inputType="textPersonName"
                android:textAlignment="center" />

            <CheckBox
                android:id="@+id/c2"
                android:layout_width="match_parent"
                android:layout_height="20dp"
                android:text="@string/Dont_Know" />

            <EditText
                android:id="@+id/a3"
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:layout_weight="1"
                android:ems="10"
                android:gravity="center_horizontal"
                android:hint="8"
                android:inputType="textPersonName"
                android:textAlignment="center" />

            <CheckBox
                android:id="@+id/c3"
                android:layout_width="match_parent"
                android:layout_height="20dp"
                android:text="@string/Dont_Know" />

            <EditText
                android:id="@+id/a4"
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:layout_weight="1"
                android:ems="10"
                android:gravity="center_horizontal"
                android:hint="3.3"
                android:inputType="textPersonName"
                android:textAlignment="center" />

            <CheckBox
                android:id="@+id/c4"
                android:layout_width="match_parent"
                android:layout_height="20dp"
                android:text="@string/Dont_Know" />

            <EditText
                android:id="@+id/a5"
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:layout_weight="1"
                android:ems="10"
                android:gravity="center_horizontal"
                android:hint="3.3"
                android:inputType="textPersonName"
                android:textAlignment="center" />

            <CheckBox
                android:id="@+id/c5"
                android:layout_width="match_parent"
                android:layout_height="20dp"
                android:text="@string/Dont_Know" />

            <EditText
                android:id="@+id/a6"
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:layout_weight="1"
                android:ems="10"
                android:gravity="center_horizontal"
                android:hint="24"
                android:inputType="textPersonName"
                android:textAlignment="center" />

            <CheckBox
                android:id="@+id/c6"
                android:layout_width="match_parent"
                android:layout_height="20dp"
                android:text="@string/Dont_Know" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="350dp"
            android:layout_height="match_parent"
            android:layout_marginLeft="8dp"
            android:layout_marginRight="15dp"
            android:layout_weight="1"
            android:orientation="vertical">

            <EditText
                android:id="@+id/b1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:ems="10"
                android:gravity="center_vertical"
                android:hint="00"
                android:inputType="textPersonName"
                android:textAlignment="center"
                tools:ignore="RtlCompat" />

            <EditText
                android:id="@+id/b2"
                android:layout_width="match_parent"
                android:layout_height="33dp"
                android:layout_weight="1"
                android:ems="10"
                android:gravity="center_vertical"
                android:hint="00"
                android:inputType="textPersonName"
                android:textAlignment="center"
                tools:ignore="RtlCompat" />

            <EditText
                android:id="@+id/b3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:ems="10"
                android:gravity="center_vertical"
                android:hint="00"
                android:inputType="textPersonName"
                android:textAlignment="center"
                tools:ignore="RtlCompat" />

            <EditText
                android:id="@+id/b4"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:ems="10"
                android:gravity="center_vertical"
                android:hint="00"
                android:inputType="textPersonName"
                android:textAlignment="center"
                tools:ignore="RtlCompat" />

            <EditText
                android:id="@+id/b5"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:ems="10"
                android:gravity="center_vertical"
                android:hint="00"
                android:inputType="textPersonName"
                android:textAlignment="center"
                tools:ignore="RtlCompat" />

            <EditText
                android:id="@+id/b6"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"

                android:ems="10"
                android:gravity="center_vertical"
                android:hint="00"
                android:inputType="textPersonName"
                android:textAlignment="center"
                tools:ignore="RtlCompat" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="409dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="1dp"
        android:layout_marginLeft="1dp"
        android:layout_marginEnd="1dp"
        android:layout_marginRight="1dp"
        android:layout_marginBottom="6dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/textView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="5dp"
            android:layout_weight="1"
            android:gravity="center_horizontal"
            android:text="@string/Appliances"
            android:textAlignment="center"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:layout_weight="1"
            android:gravity="center_horizontal"
            android:text="@string/Daily_Usages"
            android:textAlignment="center"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:layout_marginRight="15dp"
            android:layout_weight="1"
            android:gravity="center_horizontal"
            android:text="@string/Quantity"
            android:textAlignment="center"
            android:textStyle="bold" />
    </LinearLayout>

    <RelativeLayout
        android:layout_width="409dp"
        android:layout_height="87dp"
        android:layout_marginStart="1dp"
        android:layout_marginLeft="1dp"
        android:layout_marginEnd="1dp"
        android:layout_marginRight="1dp"
        android:layout_marginBottom="2dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout">

        <ImageView
            android:id="@+id/imageView7"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:scaleType="centerCrop"
            app:srcCompat="@drawable/solar2" />

        <ImageButton
            android:id="@+id/button"
            android:layout_width="65dp"
            android:layout_height="80dp"
            android:layout_centerInParent="true"
            android:background="@drawable/sunr"
            android:onClick="onClick"
            android:scaleType="centerCrop"
            tools:ignore="UsingOnClickInXml" />


    </RelativeLayout>


</androidx.constraintlayout.widget.ConstraintLayout>